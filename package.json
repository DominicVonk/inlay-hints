{
  "name": "inlay-hints",
  "displayName": "InlayHints",
  "description": "Inlay Hints",
  "version": "0.0.3",
  "preview": true,
  "enabledApiProposals": [
    "inlayHints"
  ],
  "engines": {
    "vscode": "^1.63.0"
  },
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    },
    "virtualWorkspaces": true
  },
  "publisher": "DominicVonk",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DominicVonk/inlay-hints.git"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "publish": "vsce publish --noVerify"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "16.11.11",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "tree-sitter-php": "^0.19.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "vsce": "^2.5.1",
    "vscode-test": "^1.6.1",
    "web-tree-sitter": "^0.19.4",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  }
}